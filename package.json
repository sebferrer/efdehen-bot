{
    "name": "discord-bot-template-ts",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "start:dev": "vite-node src/main.ts",
        "build": "rm -rf ./dist/ && tsc && chmod +x dist/main.js",
        "start": "node --experimental-specifier-resolution=node ./dist/main.js",
        "register-commands:dev": "vite-node src/register-commands.ts",
        "register-commands": "node --experimental-specifier-resolution=node ./dist/register-commands.js",
        "http-server:dev": "vite-node src/server.ts",
        "http-server": "node --experimental-specifier-resolution=node ./dist/server.js"
    },
    "devDependencies": {
        "@api-typings/discord": "^6.0.0",
        "@types/cors": "^2.8.13",
        "@types/cron": "^2.0.0",
        "@types/express": "^4.17.17",
        "@types/node": "^18.11.18",
        "typescript": "^4.9.3"
    },
    "author": "sebferrer",
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "cron": "^2.1.0",
        "discord.js": "^14.7.1",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "injection-js": "^2.4.0",
        "moment-mini": "^2.29.4",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0",
        "vite-node": "^0.27.0"
    }
}
